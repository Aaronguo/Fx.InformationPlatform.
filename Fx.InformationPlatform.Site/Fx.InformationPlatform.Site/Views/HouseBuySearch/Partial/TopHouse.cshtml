@model List<Fx.Entity.FxHouse.HouseBuyInfo>
@foreach (var item in Model)
{
    <div class="rows_wks_sr_list">
        <div class="right_details_wkss" style="width: 750px;">
            <div class="tops_tddds">
                <div class="tops_tddds_names"><a href="/HouseBuyDetail/Index/@item.HouseBuyInfoId" target="_blank">@item.PublishTitle</a><font>图</font></div>
                <div class="shows_price_or_exchange_ico">@item.Price</div>
                <div class="date_wks_sr_list">@item.CreatedTime.ToString("yy-MM-dd")</div>
                <div class="right_icon_show_hidden">
                    置顶推荐
                </div>
            </div>
            <div class="btm_descriptions">@item.PublishTitle</div>
        </div>
        <div class="clear"></div>
    </div>
}
