@model Fx.InformationPlatform.Site.ViewModel.GoodsBuySearchModel
@{
    ViewBag.Title = "二手转让信息";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="interval_20"></div>
<div class="grid_24">
    @Html.Partial("Partial/GoodsBuy/Search")
    <div class="interval_20"></div>


    <div class="grid_20 alpha">
        <div class="searchs-results_list">
            <div class="tops_tit_s_r">
                @Html.ActionLink("转让", "GoodsTransferSearch", new { goodsTransferSearchKey = "", page = 1, @class = "zhuanrang-a" })
            </div>
            <div class="srl_cons">
                @Html.Partial("Partial/GoodsBuy/Ad")
                @Html.Partial("Partial/GoodsBuy/TopGoods", Model.TopGoods)
                @Html.Partial("Partial/GoodsBuy/MainGoods", Model.MainGoods)
            </div>
            <div class="interval_30"></div>
            <div class="pages_index_fenye_common_wks">
                <div class="inners_paging_wks">
                    @Html.Partial("Partial/GoodsBuy/Link", Model)
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="grid_4 omega" style="float: right;">
        @Html.Partial("Partial/GoodsBuy/RightGoods", Model.RightGoods)
    </div>
    <div class="clear"></div>
</div>
<div class="clear"></div>
<div class="interval_30"></div>
