@model Fx.InformationPlatform.Site.ViewModel.HouseTransferSearchModel
@{
    ViewBag.Title = "房屋转让信息";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="interval_20"></div>
<div class="grid_24">
    @Html.Partial("Partial/HouseTransfer/Search", Model)
    <div class="interval_20"></div>


    <div class="grid_20 alpha">
        <div class="searchs-results_list">
            <div class="tops_tit_s_r">
                @Html.ActionLink("求购", "HouseBuySearch", new
                    {
                        houseTransferSearchKey = "",
                        page = 1,
                        area = Model.AreaId,
                        city = Model.CityId,
                        clc = 0
                    })
            </div>
            <div class="srl_cons">
                @Html.Partial("Partial/HouseTransfer/Ad")
                @Html.Partial("Partial/HouseTransfer/TopHouse", Model.TopHouse)
                @Html.Partial("Partial/HouseTransfer/MainHouse", Model.MainHouse)
            </div>
            <div class="interval_30"></div>
            <div class="pages_index_fenye_common_wks">
                <div class="inners_paging_wks">
                    @Html.Partial("Partial/HouseTransfer/Link", Model)
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="grid_4 omega" style="float: right;">
        @Html.Partial("Partial/HouseTransfer/RightHouse")
    </div>
    <div class="clear"></div>
</div>
<div class="clear"></div>
<div class="interval_30"></div>
