@model Fx.InformationPlatform.Site.ViewModel.HomeCarModel
<div class="grid_24">
    <div class="grid_5 alpha">
        <div class="hot_tags">
            <div class="thing_deal_tops">
                <img src="@Fx.InformationPlatform.Site.AppSettings.ImageDomain("car.png")" />
            </div>
            <div class="interval_line_c"></div>
            <div class="thing_deal_hot_tag">
                <div class="tits">热门标签</div>
                <div class="tdht_cons">
                    <a href="#">宝马</a><a href="#">奥迪</a><a href="#">奔驰</a><a href="#">丰田</a>
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="grid_1 width_cus">-</div>

    <!--最新-->
    <div class="grid_6">
        <div class="topLoop" id="sliders_4" style="height: 304px; width: 230px;">
            <div class="hd"><a class="next"></a><a class="prev"></a></div>
            <div class="bd">
                <ul class="picList">
                    @foreach (var item in Model.CarTransferInfo1s)
                    {
                        foreach (var picture in item.Pictures)
                        {
                            if (picture.TransferPictureCatagroy == (int)Fx.Entity.PictureCatagroy.Head)
                            {
                        <li>
                            <div class="pic">
                                <div class="img_cons_wks_common">
                                    <div class="tits_btm_coms_index">最新发布</div>
                                    <div class="interval_line_c"></div>
                                    <a href="@Fx.InformationPlatform.Site.AppSettings.CarTransferLink(item.CarTransferInfoId)" class="imgs_a"  target="_blank" >
                                        <img src="@Fx.InformationPlatform.Site.AppSettings.ImageUploadCdnDomain(picture.ImageUrl)" />
                                        <img src="Content/images/test1.jpg" />
                                    </a>
                                    <div class="tits_btm_coms_index names_btm">
                                        <a href="@Fx.InformationPlatform.Site.AppSettings.CarTransferLink(item.CarTransferInfoId)"  target="_blank" >
                                            @item.Price、@item.PublishTitle
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                                break;
                            }
                        }
                    }
                </ul>
            </div>
        </div>
    </div>

    <!--热门-->
    <div class="grid_6">
        <div class="topLoop" id="sliders_5" style="height: 304px; width: 230px;">
