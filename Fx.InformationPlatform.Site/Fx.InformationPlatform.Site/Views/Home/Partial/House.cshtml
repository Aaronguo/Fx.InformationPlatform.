@model Fx.InformationPlatform.Site.ViewModel.HomeHouseModel
<div class="grid_24">
    <!--标签-->
    <div class="grid_5 alpha">
        <div class="hot_tags">
            <div class="thing_deal_tops">
                <img src="@Fx.InformationPlatform.Site.AppSettings.ImageDomain("house.png")" />
            </div>
            <div class="interval_line_c"></div>
            <div class="thing_deal_hot_tag">
                <div class="tits">热门标签</div>
                <div class="tdht_cons">
                    @Html.ActionLink("House", "HouseTransferSearch", "Search", new
                            {
                                page = 1,
                                key = "",
                                area = 0,
                                city = 0,
                                clc = 47
                            }, null)
                    @Html.ActionLink("Flat", "HouseTransferSearch", "Search", new
                            {
                                page = 1,
                                key = "",
                                area = 0,
                                city = 0,
                                clc = 48
                            }, null)
                    @Html.ActionLink("学生公寓", "HouseTransferSearch", "Search", new
                            {
                                page = 1,
                                key = "",
                                area = 0,
                                city = 0,
                                clc = 49
                            }, null)
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>

    <div class="grid_1">&nbsp;</div>

    <!--最新-->
    <div class="grid_6">
        <div class="topLoop" id="sliders_7" style="height: 304px; width: 230px;">
            <div class="hd"><a class="next"></a><a class="prev"></a></div>
            <div class="bd">
                <ul class="picList">
                    @foreach (var item in Model.HouseTransferInfo1s)
                    {
                        foreach (var picture in item.Pictures)
                        {
                            if (picture.TransferPictureCatagroy == (int)Fx.Entity.PictureCatagroy.Head)
                            {
                        <li>
                            <div class="pic">
                                <div class="img_cons_wks_common">
                                    <div class="tits_btm_coms_index">最新发布</div>
                                    <div class="interval_line_c"></div>
                                    <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)" class="imgs_a"  target="_blank" >
                                        <img src="@Fx.InformationPlatform.Site.AppSettings.ImageUploadCdnDomain(picture.ImageUrl)" />

                                    </a>
                                    <div class="tits_btm_coms_index names_btm">
                                        <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)"  target="_blank" >
                                            @if (item.PublishTitle != null && item.PublishTitle.Length >= 8)
                                            {
                                                @(item.PublishTitle.Substring(0,8)+"...")                                                                                                
                                            }
                                            、
                                            ￡@item.Price
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                                break;
                            }
                        }
                    }
                </ul>
            </div>
        </div>

    </div>

    <!--热门-->
    <div class="grid_6">
        <div class="topLoop" id="sliders_8" style="height: 304px; width: 230px;">
            <div class="hd"><a class="next"></a><a class="prev"></a></div>
            <div class="bd">
                <ul class="picList">
                    @foreach (var item in Model.HouseTransferInfo2s)
                    {
                        foreach (var picture in item.Pictures)
                        {
                            if (picture.TransferPictureCatagroy == (int)Fx.Entity.PictureCatagroy.Head)
                            {
                        <li>
                            <div class="pic">
                                <div class="img_cons_wks_common">
                                    <div class="tits_btm_coms_index">最新发布</div>
                                    <div class="interval_line_c"></div>
                                    <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)" class="imgs_a"  target="_blank" >
                                        <img src="@Fx.InformationPlatform.Site.AppSettings.ImageUploadCdnDomain(picture.ImageUrl)" />

                                    </a>
                                    <div class="tits_btm_coms_index names_btm">
                                        <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)"  target="_blank" >
                                            @if (item.PublishTitle != null && item.PublishTitle.Length >= 8)
                                            {
                                                @(item.PublishTitle.Substring(0,8)+"...")                                                                                                
                                            }
                                            、
                                            ￡@item.Price
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                                break;
                            }
                        }
                    }
                </ul>
            </div>
        </div>
    </div>

    <!--置顶-->
    <div class="grid_6 omega">
        <div class="topLoop" id="sliders_9" style="height: 304px; width: 230px;">
            <div class="hd"><a class="next"></a><a class="prev"></a></div>
            <div class="bd">
                <ul class="picList">
                    @foreach (var item in Model.HouseTransferInfo3s)
                    {
                        foreach (var picture in item.Pictures)
                        {
                            if (picture.TransferPictureCatagroy == (int)Fx.Entity.PictureCatagroy.Head)
                            {
                        <li>
                            <div class="pic">
                                <div class="img_cons_wks_common">
                                    <div class="tits_btm_coms_index">最新发布</div>
                                    <div class="interval_line_c"></div>
                                    <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)" target="_blank" class="imgs_a">
                                        <img src="@Fx.InformationPlatform.Site.AppSettings.ImageUploadCdnDomain(picture.ImageUrl)" />

                                    </a>
                                    <div class="tits_btm_coms_index names_btm">
                                        <a href="@Fx.InformationPlatform.Site.AppSettings.HouseTransferLink(item.HouseTransferInfoId)"  target="_blank" >
                                           @if (item.PublishTitle != null && item.PublishTitle.Length >= 8)
                                            {
                                                @(item.PublishTitle.Substring(0,8)+"...")                                                                                                
                                            }
                                            、
                                            ￡@item.Price
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </li>
                                break;
                            }
                        }
                    }
                </ul>
            </div>
        </div>
    </div>
    <div class="clear"></div>
</div>
