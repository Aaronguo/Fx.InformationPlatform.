@model Fx.InformationPlatform.Site.ViewModel.GoodsTransferSearchModel
@{
    ViewBag.Title = "二手求购";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="interval_20"></div>
<div class="grid_24">
    <div class="grid_20 alpha">
        <div class="searchs-results_list">
            <div class="srl_cons">
                @Html.Partial("Partial/Ad")
                @Html.Partial("Partial/TopGoods", Model.TopGoods)
                @Html.Partial("Partial/MainGoods", Model.MainGoods)
            </div>
            <div class="interval_30"></div>
            <div class="pages_index_fenye_common_wks">
                <div class="inners_paging_wks">
                    @Html.Partial("Partial/Link", Model)
                    <div class="clear"></div>
                </div>
            </div>
        </div>
    </div>
    <div class="grid_4 omega" style="float: right;">
        @Html.Partial("Partial/RightGoods",Model.RightGoods)
    </div>
    <div class="clear"></div>
</div>
<div class="clear"></div>
<div class="interval_30"></div>
